<application>
  <component name="FileTypeManager" version="17">
    <ignoreFiles list="*.hprof;*.pyc;*.pyo;*.rbc;*~;.DS_Store;.git;.hg;.svn;CVS;RCS;SCCS;__pycache__;_svn;rcs;" />
    <extensionMap>
      <mapping ext="markdown" type="Markdown" />
      <mapping ext="mdown" type="Markdown" />
      <mapping ext="new" type="PHP" />
      <mapping ext="backup" type="PHP" />
      <mapping pattern=".htpasswd" type="PLAIN_TEXT" />
      <removed_mapping ext="rb" type="PLAIN_TEXT" />
      <removed_mapping ext="cgi" type="PLAIN_TEXT" />
    </extensionMap>
  </component>
</application>