<application>
  <component name="CodeInsightSettings">
    <option name="SMART_BACKSPACE" value="2" />
  </component>
  <component name="DaemonCodeAnalyzerSettings">
    <option name="SHOW_METHOD_SEPARATORS" value="true" />
  </component>
  <component name="DefaultFont">
    <option name="FONT_SIZE" value="17" />
    <option name="FONT_FAMILY" value="Fira Code Light" />
    <option name="LINE_SPACING" value="0.9" />
    <option name="USE_LIGATURES" value="true" />
  </component>
  <component name="PhpCodeFoldingSettings">
    <option name="COLLAPSE_IMPORTS" value="false" />
  </component>
  <component name="TodoConfiguration">
    <pattern pattern="\b(todo|fixme)\b.*" />
  </component>
</application>