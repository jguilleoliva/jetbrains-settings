<application>
  <component name="EditorSettings">
    <option name="STRIP_TRAILING_SPACES" value="Whole" />
    <option name="IS_ENSURE_NEWLINE_AT_EOF" value="true" />
    <option name="ARE_LINE_NUMBERS_SHOWN" value="true" />
  </component>
  <component name="TodoConfiguration">
    <pattern icon="default" useCustomColors="false" case-sensitive="true" pattern="\b@TODO\b.*">
      <option name="FOREGROUND" value="b15703" />
      <option name="FONT_TYPE" value="2" />
    </pattern>
    <pattern icon="default" useCustomColors="false" case-sensitive="true" pattern="\b@FIXME\b.*">
      <option name="FOREGROUND" value="b15703" />
      <option name="FONT_TYPE" value="2" />
    </pattern>
    <pattern icon="default" useCustomColors="false" case-sensitive="true" pattern="&lt;!---@TODO.*">
      <option name="FOREGROUND" value="b15703" />
      <option name="FONT_TYPE" value="2" />
    </pattern>
  </component>
</application>